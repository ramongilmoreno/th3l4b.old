#
# Shopping model
#

define name "Name";
define string "string";

model "Shopping"
	properties {
	};
	
entity "Item" {
	field @name @string;
	field "Description" @string;
};

entity "Store" {
	field @name @string;
	field "Description" @string;
};

entity "Need" {
	field "Quantity" "decimal";
};

relationship "Need" "Item" many-to-one;
