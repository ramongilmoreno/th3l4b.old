model "Office"
	properties {
	};
	
	
entity "Employee" {
	field "Name" "string";
	field "Surname" "string";
};

entity "Department" {
	field "Name" "string";
};

relationship "Department" "Department" many-to-one
	direct "Parent" {}
	reverse "Children" {}
;

entity "Task" {
	field "Duration" "decimal";
};

entity "Assignation" {
	field "Complete" "decimal";
};

relationship "Employee" "Task" many-to-many "Assignation";
